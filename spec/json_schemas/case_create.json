    "document_for_collection": {
      "$id": "#/properties/data/properties/documentCollection/items",
      "type": "object",
      "title": "The Document Schema",
      "required": [
        "id",
        "value"
      ],
      "properties": {
        "value": {
          "$id": "#/properties/data/properties/documentCollection/items/properties/value",
